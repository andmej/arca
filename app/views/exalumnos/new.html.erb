<h1>New exalumno</h1>

<%= error_messages_for :exalumno %>

<% form_for(@exalumno) do |f| %>
  <p>
    <b>Fecha retiro</b><br />
    <%= f.date_select :fecha_retiro %>
  </p>

  <p>
    <b>Motivo</b><br />
    <%= f.text_area :motivo %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', exalumnos_path %>
