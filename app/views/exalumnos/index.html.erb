<h1>Listing exalumnos</h1>

<table>
  <tr>
    <th>Fecha retiro</th>
    <th>Motivo</th>
  </tr>

<% for exalumno in @exalumnos %>
  <tr>
    <td><%=h exalumno.fecha_retiro %></td>
    <td><%=h exalumno.motivo %></td>
    <td><%= link_to 'Show', exalumno %></td>
    <td><%= link_to 'Edit', edit_exalumno_path(exalumno) %></td>
    <td><%= link_to 'Destroy', exalumno, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New exalumno', new_exalumno_path %>
